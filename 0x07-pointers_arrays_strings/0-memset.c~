#include "main.h"

/**
 *_memset - fills memory with a constant byte
 * @s: memory area to be filled
 *@b: constant byte
 *@n: number of times to fill memory
 *Return: pointer to memory area s
 */

char *_memset(char *s, char b, unsigned int n)
{
	char *ans = s;

	if (s == NULL)
		return (s);
		while (n)
		{
			*s = b;
			s++;
			n--;
		}
	return(ans);
}
